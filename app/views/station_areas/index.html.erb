<%- model_class = StationArea -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:location) %></th>
      <th><%= model_class.human_attribute_name(:station_name) %></th>
      <th><%= model_class.human_attribute_name(:etod_score) %></th>
      <th><%= model_class.human_attribute_name(:vehicle_miles_traveled) %></th>
      <th><%= model_class.human_attribute_name(:transit_commute_share) %></th>
      <th><%= model_class.human_attribute_name(:vehicle_ownership) %></th>
      <th><%= model_class.human_attribute_name(:transportation_ghgs) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @station_areas.each do |station_area| %>
      <tr>
        <td><%= link_to station_area.id, station_area_path(station_area) %></td>
        <td><%= station_area.location %></td>
        <td><%= station_area.station_name %></td>
        <td><%= station_area.etod_score %></td>
        <td><%= station_area.vehicle_miles_traveled %></td>
        <td><%= station_area.transit_commute_share %></td>
        <td><%= station_area.vehicle_ownership %></td>
        <td><%= station_area.transportation_ghgs %></td>
        <td><%=l station_area.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_station_area_path(station_area), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      station_area_path(station_area),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_station_area_path,
            :class => 'btn btn-primary' %>
